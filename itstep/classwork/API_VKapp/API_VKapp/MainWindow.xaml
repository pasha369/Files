<Window x:Class="API_VKapp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel>
            <WebBrowser Name="WebBrowserWindow" Width="350" Height="200"> 
            </WebBrowser>
            <WrapPanel>
                <StackPanel Margin="25 5">
                    <Button Name="btnQuery" Click="BtnQuery_OnClick" Width="200">query</Button>
                    <TextBox Name="txtOut" Width="200" Height="90" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto"></TextBox>
                </StackPanel>
                <StackPanel Margin="25 5">
                    <Button Click="ButtonBase_OnClick" Width="200">deserialize</Button>
                    <TextBox Name="txtNames" Width="200" Height="90" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto"></TextBox>
                </StackPanel>
            </WrapPanel>
        </StackPanel>
    </Grid>
</Window>
